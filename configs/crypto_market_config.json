{
  "name": "数字货币市场配置",
  "description": "币安交易所加密货币交易配置",
  "market": "crypto",
  "agent_type": "BaseAgentCrypto",
  "exchange": "binance",
  "enabled": true,
  "date_range": {
    "init_date": "2025-10-01",
    "end_date": "2025-10-21"
  },
  "models": [
    {
      "name": "claude-3.7-sonnet",
      "basemodel": "anthropic/claude-3.7-sonnet",
      "signature": "claude-3.7-sonnet",
      "enabled": true,
      "openai_base_url": "Optional: YOUR_OPENAI_BASE_URL",
      "openai_api_key": "Optional: YOUR_OPENAI_API_KEY"
    },
    {
      "name": "deepseek-chat-v3.1",
      "basemodel": "deepseek/deepseek-chat-v3.1",
      "signature": "deepseek-chat-v3.1",
      "enabled": false
    },
    {
      "name": "qwen3-max",
      "basemodel": "qwen/qwen3-max",
      "signature": "qwen3-max",
      "enabled": false
    },
    {
      "name": "gemini-2.5-flash",
      "basemodel": "google/gemini-2.5-flash",
      "signature": "gemini-2.5-flash",
      "enabled": false
    },
    {
      "name": "gpt-5",
      "basemodel": "openai/gpt-5",
      "signature": "gpt-5",
      "enabled": false
    }
  ],
  "agent_config": {
    "max_steps": 30,
    "max_retries": 3,
    "base_delay": 1.0,
    "initial_cash": 10000.0,
    "testnet_enabled": true
  },
  "data_source": "binance",
  "api_key_required": ["binance", "binance_secret", "openai"],
  "prompt_template": "crypto_market_prompt",
  "trading_rules": {
    "market_hours": "24/7",
    "settlement": "实时",
    "min_order_size": 0.001,
    "supported_pairs": ["BTCUSDT", "ETHUSDT", "BNBUSDT", "ADAUSDT", "DOTUSDT"],
    "supported_order_types": ["market", "limit", "stop_loss", "take_profit"],
    "trading_fees": {
      "spot": 0.001,
      "bnb_discount": 0.00075
    }
  },
  "risk_management": {
    "max_position_size": 0.2,
    "stop_loss_percentage": 0.05,
    "take_profit_percentage": 0.15,
    "max_daily_loss": 0.03,
    "volatility_threshold": 0.1
  },
  "log_config": {
    "log_path": "./data/agent_data_crypto",
    "log_level": "INFO",
    "save_trades": true,
    "save_portfolio": true
  }
}